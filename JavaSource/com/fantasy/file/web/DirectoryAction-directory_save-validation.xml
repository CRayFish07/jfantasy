<validators>
	<field name="key">
		<field-validator type="requiredstring">
			<message>主键不能为空!</message>
		</field-validator>
		<field-validator type="directoryKeyUnique">
			<message>主键不能重复!</message>
		</field-validator>
	</field>
	
</validators>