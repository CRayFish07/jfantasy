    <!-- 会员管理 -->
    <package name="fides-member" extends="fantasy-default" namespace="/member">
        <action name="index" class="com.fantasy.member.web.MemberAction" method="index">
            <result type="freemarker">/template/%{pageTheme}/member/member.ftl</result>
        </action>

        <action name="search" class="com.fantasy.member.web.MemberAction" method="search"/>

        <action name="edit" class="com.fantasy.member.web.MemberAction" method="edit">
            <result type="freemarker">/template/%{pageTheme}/member/member_edit.ftl</result>
        </action>

        <action name="add" class="com.fantasy.member.web.MemberAction">
            <result type="freemarker">/template/%{pageTheme}/member/member_add.ftl</result>
        </action>

        <action name="view" class="com.fantasy.member.web.MemberAction" method="edit">
            <result type="freemarker">/template/%{pageTheme}/member/member_view.ftl</result>
        </action>

        <action name="save" class="com.fantasy.member.web.MemberAction" method="save"/>

        <action name="resetpwd" class="com.fantasy.member.web.MemberAction" method="resetpwd"/>

        <action name="delete" class="com.fantasy.member.web.MemberAction" method="delete"/>
    </package>
</struts>